// Faça um programa que peça 4 notas e calcule a média. Se a média for maior ou igual a 5, exibir "aprovado" senão exibir "reprovado"

let nota1 = 2
let nota2 = 6
let nota3 = 1
let nota4 = 8
media = ( nota1 + nota2 + nota3 + nota4 )/4
if(media >= 5){
    console.log("Aprovado")
}else if(media < 5){
    console.log("Reprovado")
}